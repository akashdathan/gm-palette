{
  "name": "@davidegheri/gm-palette",
  "version": "4.0.0-beta.0",
  "description": "Dominant color and palette using graphicsmagick.",
  "main": "./dist/node/index.js",
  "typings": "./dist/types/index.d.ts",
  "module": "./dist/src/index.js",
  "bin": {
    "gm-palette": "./dist/node/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build:clean": "rimraf ./dist",
    "build:es": "tsc -p tsconfig.es.json",
    "build:node": "tsc -p tsconfig.node.json",
    "build": "yarn build:clean && yarn build:node && yarn build:es",
    "preversion": "yarn test && yarn lint",
    "version": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davide-Gheri/gm-palette.git"
  },
  "keywords": [
    "dominant color",
    "colors",
    "top colors",
    "palette",
    "image colors",
    "gm colors",
    "gm palette",
    "graphicsmagick"
  ],
  "contributors": [
    "Akash Dathan",
    "Davide Gheri"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Davide-Gheri/gm-palette/issues"
  },
  "homepage": "https://github.com/Davide-Gheri/gm-palette#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "gm": "^1.23.1"
  },
  "devDependencies": {
    "@types/gm": "^1.18.2",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.92",
    "@types/node": "^12.0.0",
    "coveralls": "^3.0.3",
    "jest": "^24.1.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
